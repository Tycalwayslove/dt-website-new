{
  "name": "dt-website-new",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "dev:test": "vite --mode test",
    "build": "vite build",
    "build:test": "vite build --mode test",
    "build:prod": "vite build --mode production",
    "preview": "vite preview --port 5173",
    "preview:test": "vite preview --mode test --port 5176 --strictPort",
    "preview:prod": "vite preview --mode production --port 5177 --strictPort",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "deploy": "node scripts/deploy.mjs",
    "deploy:clean": "node scripts/deploy.mjs --clean",
    "upload:oss:test": "node scripts/oss-upload.mjs --env=test",
    "upload:oss:prod": "node scripts/oss-upload.mjs --env=prod",
    "deploy:oss:test": "pnpm build:test && pnpm upload:oss:test",
    "deploy:oss:prod": "pnpm build:prod && pnpm upload:oss:prod"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
